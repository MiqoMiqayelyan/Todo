{
    "extends": ["react-app", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y"],
    "rules": {
        "no-console":"warn",        
        "quotes": ["error", "single",{ "allowTemplateLiterals": true }],
        "jsx-quotes": ["error", "prefer-double"],
        "indent":["warn",2],
        "semi":["warn","always"]

    }
}